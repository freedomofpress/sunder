{
  "presets": ["env", "stage-0", "react"],
  "plugins": ["add-module-exports"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "plugins": [
        ["webpack-loaders", { "config": "webpack.config.node.js", "verbose": false }],
        ["module-resolver", {
          "alias": {
            "electron": "./test/mocks/electron.js"
          }
        }]
      ]
    }
  }
}
